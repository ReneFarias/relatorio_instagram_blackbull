	measure Visu/Alcance =
			
			VAR visuAlcance = SUM(Metricas[Alcance])
			VAR midia = SELECTEDVALUE(Publicacoes[Midia])
			VAR nome = IF(midia = "VIDEO", "Visualizações", "Alcance")
			VAR valorTexto = IF(NOT ISBLANK(visuAlcance), FORMAT(visuAlcance, "#,##0"), "Sem dados")
			RETURN
			nome & ":   " & valorTexto
		lineageTag: 4acbf1d3-82f7-4d24-868b-b58eed950e96

