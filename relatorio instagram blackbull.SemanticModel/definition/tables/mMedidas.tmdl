	measure 'Tempo Total de Visualizações de Video' =
			
			IF (
			    SELECTEDVALUE(Publicacoes[Midia]) = "IMAGE",
			    BLANK(),
			    VAR ms = SUM('Metricas'[Tempo Tot Visualizacao Videos (Milissegundos)])
			    VAR h = QUOTIENT(ms, 3600000)
			    VAR m = QUOTIENT(MOD(ms, 3600000), 60000)
			    VAR s = QUOTIENT(MOD(ms, 60000), 1000)
			    RETURN
			        FORMAT(h, "0") & ":" &
			        FORMAT(m, "00") & ":" &
			        FORMAT(s, "00")
			)
		lineageTag: 165b4bea-05d2-4135-9392-98f3ddb0732e

