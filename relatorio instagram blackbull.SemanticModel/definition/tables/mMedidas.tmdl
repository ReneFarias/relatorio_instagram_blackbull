	measure 'Publi Video' =
			
			VAR qtde = CALCULATE(
			    DISTINCTCOUNT(Publicacoes[ID]),
			    Publicacoes[Midia] = "VIDEO"
			)
			RETURN
			IF(
			    ISBLANK(qtde),
			    "VAZIO",
			    qtde
			)
