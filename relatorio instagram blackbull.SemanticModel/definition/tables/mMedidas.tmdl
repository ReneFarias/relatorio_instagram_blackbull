	measure VideoStoriesHTML = ```
			
			VAR link = SELECTEDVALUE(Stories[URL Midia])
			VAR tipoMidia = SELECTEDVALUE(Stories[Midia])
			VAR defaultHTML = "<div style='width:360px;height:640px;display:flex;align-items:center;justify-content:center;background-color:#f0f0f0;color:#888;font-size:20px;font-weight:bold;'>SELECIONE A MÍDIA</div>"
			
			RETURN
			IF (
			    NOT ISBLANK(link),
			    IF (
			        tipoMidia = "VIDEO",
			        "<video width='360' height='640' autoplay muted controls>
			            <source src='" & link & "' type='video/mp4'>
			         </video>",
			        "<img src='" & link & "' style='max-width:360px;height:640px;'>"
			    ),
			    defaultHTML
			)
			
			```
		lineageTag: d452d21c-e3ed-4293-abbd-74e33287347a

	measure VideoHTML =
			
			VAR link = SELECTEDVALUE(APIpublicacoes[media_url])
			VAR img = SELECTEDVALUE(Publicacoes[Midia])
			VAR defaultHTML = "<div style='width:360px;height:640px;display:flex;align-items:center;justify-content:center;background-color:#f0f0f0;color:#888;font-size:20px;font-weight:bold;'>SELECIONE A MÍDIA</div>"
			RETURN
			IF (
			    NOT ISBLANK ( link ),
			    IF( img = "VIDEO",
			    "<video width=""360"" height=""640"" autoplay muted controls>" &
			    "<source src=""" & link & """ type=""video/mp4""></video>",
			    /* HTML para IMAGEM */
			        "<img src=""" & link & """ style=""max-width:360px;height:540;padding:5em 16px;"">"
			), defaultHTML)
		lineageTag: d4ff6727-68b8-477e-98ec-171fc65bf8e7

