		lineageTag: 643cb870-2385-4b8f-9b87-bfa81bdac610
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column NomeSemana = SWITCH(WEEKDAY(dCalendario[Data]), 1, "DOM", 2, "SEG", 3, "TER", 4, "QUA", 5, "QUI", 6, "SEX", 7, "SÁB")
